#include <stdio.h>
int main() {
    int num,n, remainder,arm= 0;
    printf("Enter a number: ");
    scanf("%d", &n);
	 n= num;
    while (num!= 0) {
        remainder = n% 10;
        arm += remainder * remainder * remainder;
        num/= 10;
    }
    if (arm== n)
        printf("%d is an Armstrong number.\n",num);
    else
        printf("%d is not an Armstrong number.\n"),num;
        
}
